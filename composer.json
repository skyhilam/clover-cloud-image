{
  "name": "clover/cloud-image",
  "description": "A cloud image",
  "license": "MIT",
  "keywords": [
    "cloud",
    "image"
  ],
  "type": "library",
  "authors": [
    {
      "name": "",
      "email": ""
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.8.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "orchestra/testbench": "~3.8.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Clover\\CloudImage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Clover\\CloudImage\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Clover\\CloudImage\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
